<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.collegeek.scholoship" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme"
        android:versionCode="4"
        android:versionName="1.4" >
        <activity
            android:name="collegeek.example.collegeek.scholoship.activity.MainActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="collegeek.example.collegeek.scholoship.activity.MainTabActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="collegeek.example.collegeek.scholoship.activity.CollegeDetailActivity"
            android:label="@string/title_activity_college_detail"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="collegeek.example.collegeek.scholoship.activity.NoResultActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait">
        </activity>
    </application>

</manifest>
